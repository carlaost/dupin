{
  "name": "Dupin",
  "description": "Tracks your academic reading",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "o.png"
  },
  "content_scripts": [
    {
        "js": ["scripts/content.js"],
        "matches": ["<all_urls>"]
    }
    ]
}